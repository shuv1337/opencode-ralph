{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "darkBg": "#000000",
    "darkBgAlt": "#0a0f0a",
    "darkBgPanel": "#0d1a0d",
    "darkFg": "#00ff41",
    "darkFgMuted": "#2e5a32",
    "darkRed": "#ff3030",
    "darkPink": "#ff6b9d",
    "darkOrange": "#7fff00",
    "darkYellow": "#b5ff00",
    "darkGreen": "#00ff41",
    "darkCyan": "#00ffc8",
    "darkBlue": "#00ff9f",
